    "image-webpack-loader": "4.6.0",
    "imports-loader": "0.8.0",
    "interpolate-html-plugin": "^3.0.0",
    "jest-cli": "24.7.1",
    "jest-dom": "3.1.3",
    "jest-styled-components": "6.3.1",
    "lint-staged": "8.1.5",
